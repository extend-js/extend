{
  "name": "@roshin/extend",
  "private": true,
  "version": "0.0.1",
  "description": "一个轻量级 JS/TS 扩展工具库",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap.js",
    "build": "node ./scripts/build.js",
    "scripts": "rollup -c ./rollup.config.js",
    "ls-lint": "ls-lint",
    "lint": "eslint --ext .js,.ts --ignore-path .eslintignore ./ --max-warnings 0",
    "fix": "run-s fix:*",
    "fix:lint": "eslint --ext \"./**/*{.ts,.js}?(x)\" --fix",
    "fix:json": "prettier --write --parser json \"./**/*.json\"",
    "prettier": "prettier --write --parser typescript \"./**/*{.ts,.js}?(x)\"",
    "test": "jest --runInBand --verbose",
    "test:coverage": "jest --coverage --verbose -u --bail",
    "codecov": "codecov",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "doc": "typedoc dist/ --target ES6 --mode file --out docs --externalPattern",
    "doc:json": "typedoc dist/ --target ES6 --mode file --json docs/typedoc.json",
    "release": "node scripts/release.js",
    "preinstall": "node ./scripts/checkYarn.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "cz"
  },
  "keywords": [
    "extend",
    "JavaScript",
    "TypeScript",
    "tools",
    "utils",
    "rollup",
    "lerna",
    "roshin"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run ls-lint && npm run lint",
      "pre-receive": "npm run format"
    }
  },
  "lint-staged": {
    "./**/*{.ts,.js}?(x)": [
      "npm run lint",
      "npm run fix:prettier"
    ],
    "./**/*.json": [
      "npm run fix:json",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@babel/runtime-corejs3": "^7.12.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt": "^11.0.0",
    "@ls-lint/ls-lint": "^1.9.2",
    "@microsoft/api-extractor": "^7.11.4",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/brotli": "^1.3.0",
    "@types/fs-extra": "^9.0.5",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vuepress/plugin-pwa": "^1.7.1",
    "babel-eslint": "^10.1.0",
    "brotli": "^1.3.2",
    "chalk": "^4.1.0",
    "codecov": "^3.8.1",
    "commitizen": "^4.2.2",
    "conventional-changelog-cli": "^2.1.1",
    "core-js": "2.x",
    "cz-customizable": "^6.3.0",
    "enquirer": "^2.3.6",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "execa": "^4.1.0",
    "fs-extra": "^9.0.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.1",
    "minimist": "^1.2.5",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rollup": "^2.33.3",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "semver": "^7.3.4",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typedoc-plugin-markdown": "^3.0.11",
    "typescript": "^4.0.5",
    "vuepress": "^1.7.1",
    "vuepress-plugin-code-copy": "^1.0.6",
    "vuepress-plugin-nprogress": "^1.1.8",
    "vuepress-plugin-sitemap": "^2.3.1",
    "vuepress-theme-antdocs": "^1.2.2",
    "zlib": "^1.0.5"
  }
}
